    trailingComma: "all",
    tabWidth: 4,
    printWidth: 140,
    useTabs: false,
    semi: true,
    singleQuote: false,
    quoteProps: "as-needed",
    bracketSpacing: true,
    arrowParens: "always",
    jsxBracketSameLine: false,
    endOfLine: "auto",
    overrides: [
        {
            files: ".prettierrc",
            options: {
                parser: "json",
            },
        },
    ],
}
